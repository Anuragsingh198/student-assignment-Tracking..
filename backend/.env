# App Environment
NODE_ENV=development
PORT=5000

# Database
MONGODB_URI=


# JWT Configuration
JWT_ACCESS_SECRET=supersecret_access_key_with_at_least_32_characters_123
JWT_REFRESH_SECRET=supersecret_refresh_key_with_at_least_32_characters_456
JWT_ACCESS_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# Cookies
COOKIE_SECRET=cookie_secret_key_789

# CORS
CORS_ORIGIN=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# AI Service (optional)
AI_SERVICE_URL=https://api.example-ai.com
AI_SERVICE_API_KEY=example_ai_api_key_123
AI_ENABLED=false

# Logging
LOG_LEVEL=info
